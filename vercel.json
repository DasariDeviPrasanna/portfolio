{   
    "version": 2,
    "builds": [
        {
            "src": "portfolio.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/portfolio.html"
        }
    ]
} 
